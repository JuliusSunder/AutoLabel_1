{
  "name": "autolabel",
  "productName": "AutoLabel",
  "version": "1.0.3",
  "description": "Automated shipping label management for resellers",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "clean": "node -e \"try { require('fs').rmSync('.vite', { recursive: true, force: true }); console.log('✓ Cleaned .vite build cache'); } catch(e) { console.log('✓ No cache to clean'); }\"",
    "fresh": "npm run clean && npm run start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "start:production": "cross-env WEBSITE_URL=https://autolabel.app VITE_WEBSITE_URL=https://autolabel.app electron-forge start",
    "make:signed": "cross-env NODE_ENV=production electron-forge make",
    "package:signed": "cross-env NODE_ENV=production electron-forge package",
    "publish": "electron-forge publish",
    "publish:github": "electron-forge publish --from-dry-run",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "JuliusSunder",
    "email": "cookius2211@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
